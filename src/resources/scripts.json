{
  "npm": {
    "build": "parcel build ./src/index.html --no-source-maps",
    "dev": "parcel watch ./src/index.html",
    "lint": "eslint \"src/**.{js,jsx,html}\"",
    "lint:fix": "eslint \"src/**.{js,jsx,html}\" --fix",
    "start": "parcel ./src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}